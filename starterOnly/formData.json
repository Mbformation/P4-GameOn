{
    "formSelector": "#myForm",
    "submitButtonSelector": ".btn-submit",
    "fields": [
      {
        "fieldSelector": "first",
        "fieldType": "text-input",
        "fieldRegex": "^[A-Za-z]+$",
        "fieldRequired": true,
        "fieldErrorMessage": "Pr√©nom Valide"
      },
      {
        "fieldSelector": "last",
        "fieldType": "text-input",
        "fieldRegex": "^[A-Za-z]+$",
        "fieldRequired": true,
        "fieldErrorMessage": "Nom Valide"
      },
      {
        "fieldSelector": "email",
        "fieldType": "text-input",
        "fieldRegex": "^\\S+@\\S+\\.\\S+$",
        "fieldRequired": true,
        "fieldErrorMessage": "Email Valide"
      },
      {
        "fieldSelector": "birthdate",
        "fieldType": "text-input",
        "fieldRegex": "^(?=.*[A-Za-z])(?=.*\\d)[A-Za-z\\d]{8,}$",
        "fieldRequired": true,
        "fieldErrorMessage": "Date Valide"
      },
      {
        "fieldSelector": "quantity",
        "fieldType": "text-input",
        "fieldRegex": "^(?=.*[A-Za-z])(?=.*\\d)[A-Za-z\\d]{8,}$",
        "fieldRequired": true,
        "fieldErrorMessage": "Entre 1 et 100"
      },
      {
        "fieldSelector": "quantity",
        "fieldType": "radio-button",
        "fieldRequired": true
      },
      {
        "fieldSelector": "quantity",
        "fieldType": "checkbox",
        "fieldRequired": true
      }
    ]
